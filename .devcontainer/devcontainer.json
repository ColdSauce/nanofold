{
	"name": "Nanofold Dev Container",
	"mounts": [
		"source=/home/ogchen/data,target=/data,type=bind,consistency=cached"
	],
	"dockerComposeFile": ["../docker-compose.yml", "docker-compose.extend.yml"],
	"service": "nanofold",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.black-formatter"
			]
		}
	}
}
